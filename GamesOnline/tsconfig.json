{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "lib": [
      "es5",
      "es2015.promise",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "skipDefaultLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "types": [ "webpack-env" ],
    "allowJs": true
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
